        <script><![CDATA[if(producer.is_this_application_associated_with_an_existing_system_security_plan_ssp == 'Yes'){
	current.system_security_plan_exists = true;
}

if(producer.was_privacy_threshold_analysis_pta_completed == 'Yes'){	
current.privacy_threshold_analysis_completed = true;
}

if(producer.was_privacy_impact_assesment_pia_completed == 'Yes'){
	current.privacy_impact_assessment_completed = true;
}

if(producer.is_your_application_entered_in_aart == 'Yes'){	
current.application_entered_in_aart = true;
}
	
var milestoneRecord = [];
	var milestoneStrings = JSON.parse(producer.milestones);
	for(var i = 0; i<milestoneStrings.length; ++i){
		var milestoneGR = new GlideRecord('x_g_nasa_grc_it_se_milestones');
		milestoneGR.initialize();
		milestoneGR.short_description = milestoneStrings[i].title;
		milestoneGR.due_date = milestoneStrings[i].due_date;
		milestoneGR.description = milestoneStrings[i].description;
		milestoneGR.u_assigned_to = milestoneStrings[i].assigned_to;
		var rec = milestoneGR.insert();
		milestoneRecord.push(rec);
	}

current.u_milestones = milestoneRecord.toString();
current.update();
]]></script>
